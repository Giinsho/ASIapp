                        <Grid Grid.Column="1" Grid.Row="2"  Margin="15,2,2,2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>

                            <RadioButton  Checked="RadioButton_Checked" GroupName="TestGroup" x:Name="test_3" VerticalAlignment="Center" HorizontalAlignment="Center">test_3</RadioButton>

                        </Grid>
                    </Grid>


                </StackPanel>
                <StackPanel Grid.Row="4" VerticalAlignment="Center">
