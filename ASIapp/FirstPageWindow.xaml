                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBox x:Name="paccB22" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB22_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.7" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Grid>

                        <Grid Grid.Column="1" Grid.Row="5" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBox x:Name="paccB23" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB23_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.5" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Grid>


                        <Grid Grid.Column="2" Grid.Row="3" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBox x:Name="paccB31" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB31_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.9" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Grid>

