                    </Grid>


                </StackPanel>

                <StackPanel Grid.Row="2" Orientation="Vertical" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Grid.Row="0" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="70" Content="n__of__A" HorizontalAlignment="Left" VerticalAlignment="Center"></Label>
                            <TextBox Text="2"  x:Name="NumberOfA" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="NumberOfA_TextChanged" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40"/>
                        </Grid>
                        <Grid Grid.Column="0"  Grid.Row ="1"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="70" Content="n__of__D" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox  Text="2" x:Name="NumberOfD" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="NumberOfD_TextChanged" Width="40" Height="26" HorizontalAlignment="Center"/>
                        </Grid>
                        <Grid   Grid.Column="0"  Grid.Row="2"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="70" Content="n__of__B" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="4" x:Name="NumberOfB" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="NumberOfB_TextChanged" Width="40" Height="26" HorizontalAlignment="Right"/>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="0"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton x:Name="rbtn_readCaStates" VerticalAlignment="Center" HorizontalAlignment="Center"></RadioButton>
                            <Button Grid.Column="1" x:Name="Btn_readCaStates" Click="Btn_readCaStates_Click">read CA__STATES</Button>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="1"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton x:Name="rbtn_readAprofile" HorizontalAlignment="Center" VerticalAlignment="Center"></RadioButton>
                            <Button Grid.Column="1"  x:Name="Btn_readAProfile" Click="Btn_readAProfile_Click">read A__PROFILE</Button>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="2"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton x:Name="rbtn_readRandNum" VerticalAlignment="Center" HorizontalAlignment="Center"></RadioButton>
                            <Button Grid.Column="1" x:Name="Btn_ReadRandNum" Click="Btn_ReadRandNum_Click">read RAND__NUM</Button>
                        </Grid>
                    </Grid>



                </StackPanel>

                <StackPanel Grid.Row="3" Orientation="Vertical" Margin="5" HorizontalAlignment="Left" VerticalAlignment="Center">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0" Grid.Row="0" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="70" Content="n__of__iter" HorizontalAlignment="Left" VerticalAlignment="Center"></Label>
                            <TextBox Text="5" x:Name="NumberOfiter" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="NumberOfiter_TextChanged" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center" Width="40"/>
