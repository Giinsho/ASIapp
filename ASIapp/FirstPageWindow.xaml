                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__HS1" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.15" x:Name="probabilityHealthState1" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityHS1_TextChanged" Width="40" Height="26" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="2,0,0,0"/>
                        </Grid>

                        <Grid   Grid.Column="1"  Grid.Row="3"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__HS2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.60" x:Name="probabilityHealthState2" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityHS2_TextChanged" Width="40" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>

                        <Grid   Grid.Column="2"  Grid.Row="3"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__HS3" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.25" x:Name="probabilityHealthState3" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityHS3_TextChanged" Width="40" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>

                        <Grid   Grid.Column="0"  Grid.Row="4"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__ill1" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.70" x:Name="probabilityIllness1" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityIll1_TextChanged" Width="40" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>

                        <Grid   Grid.Column="1"  Grid.Row="4"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__ill2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.20" x:Name="probabilityIllness2" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityIll2_TextChanged" Width="40" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>

                        <Grid   Grid.Column="2"  Grid.Row="4"  Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Label Width="50" Content="p__ill3" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox Text="0.10" x:Name="probabilityIllness3" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="probabilityIll3_TextChanged" Width="40" Height="26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Grid>

                    </Grid>



                    <Grid  Margin="2">
                        <Grid.ColumnDefinitions   >
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="50"/>
                            <ColumnDefinition Width="auto"/>
                            <ColumnDefinition Width="50"/>
                        </Grid.ColumnDefinitions>
                        <Label>n__iter__susp__B</Label>
                        <TextBox Text="1" x:Name="numberOfIterSuspB" PreviewTextInput="TextBox_PreviewNumbersOnly" Grid.ColumnSpan="3" Margin="80,0,83,0" TextChanged="numberOfIterSuspB_TextChanged" Height="26" VerticalAlignment="Top" />
