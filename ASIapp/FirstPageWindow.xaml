                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <RadioButton GroupName="TestGroup" Checked="RadioButton_Checked" x:Name="test_1" VerticalAlignment="Center" HorizontalAlignment="Center">test_1</RadioButton>
                            <RadioButton  Margin="15,0,0,0" Grid.Column="1" GroupName="SeedGroup" x:Name="RbtnClockSeed" Checked="RbtnClockSeed_Checked" VerticalAlignment="Center" HorizontalAlignment="Center">clock__seed</RadioButton>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="1"  Margin="15,2,2,2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <RadioButton GroupName="TestGroup"  Checked="RadioButton_Checked" x:Name="test_2" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="0,0,0,18" Grid.RowSpan="2">test_2</RadioButton>
                            <RadioButton Margin="15,0,0,0" Grid.Column="1" GroupName="SeedGroup"  x:Name="RbtnCustomSeed" Checked="RbtnCustomSeed_Checked" VerticalAlignment="Center" HorizontalAlignment="Center">custom__seed</RadioButton>
                            <TextBox  Margin="15,0,0,0" Grid.Column="1" Grid.Row="1"  x:Name="CustomSeedText"  PreviewTextInput="TextBox_PreviewNumbersOnly"  TextChanged="CustomSeedText_TextChanged"  IsEnabled="{Binding ElementName=RbtnCustomSeed, Path=IsChecked}"></TextBox>
