                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="8*" />
                                <ColumnDefinition Width="33*"/>
                            </Grid.ColumnDefinitions>
                            <Label Width="70" Content="n__of__exper" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                            <TextBox Text="1" x:Name="NumberOfexper" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="NumberOfexper_TextChanged" Width="40" Height="26" HorizontalAlignment="Left" Grid.ColumnSpan="2"/>
                        </Grid>

                        <Grid Grid.Column="1" Grid.Row="0"  Margin="15,2,2,2">
