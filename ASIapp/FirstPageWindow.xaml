                        </Grid>

                        <Grid Grid.Column="1" Grid.Row="0" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            <Label  Content="IQ2" HorizontalAlignment="Center" VerticalAlignment="Center"  />

