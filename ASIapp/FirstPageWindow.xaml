                        </Grid>

                        <Grid Grid.Column="0" Grid.Row="4" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <Label FontWeight="Bold"  Width="50" Content="B2" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox x:Name="paccB12" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB12_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.9" HorizontalAlignment="Right" VerticalAlignment="Center" />
                        </Grid>

                        <Grid Grid.Column="0" Grid.Row="5" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <Label FontWeight="Bold"  Width="50" Content="B3" HorizontalAlignment="Left" VerticalAlignment="Center"/>
                            <TextBox x:Name="paccB13" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB13_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.8" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Grid>


                        <Grid Grid.Column="1" Grid.Row="3" Margin="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBox x:Name="paccB21" Grid.Column="1" PreviewTextInput="TextBox_PreviewNumbersOnly" TextChanged="paccB21_TextChanged" Width="40" HorizontalContentAlignment="Center" Text="0.9" HorizontalAlignment="Center" VerticalAlignment="Center" />
